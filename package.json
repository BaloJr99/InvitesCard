    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@auth0/angular-jwt": "^5.2.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.8",
    "dotenv": "^16.4.7",
    "jquery": "^3.7.1",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.1",
    "socket.io-client": "^4.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
